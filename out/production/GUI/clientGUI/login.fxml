<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane depthTest="DISABLE" layoutX="15.0" layoutY="58.0" prefHeight="367.0" prefWidth="571.0">
         <children>
            <TextField id="nickName" fx:id="nickName" alignment="CENTER" layoutX="141.0" layoutY="136.0" promptText="Nickname">
               <font>
                  <Font size="22.0" />
               </font>
            </TextField>
            <Label fx:id="nickNameLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="161.0" layoutY="90.0" text="Enter your nickname:">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Button fx:id="logButton" layoutX="178.0" layoutY="190.0" mnemonicParsing="false" onAction="#logIn" prefHeight="36.0" prefWidth="198.0" text="Log in">
               <font>
                  <Font size="22.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
